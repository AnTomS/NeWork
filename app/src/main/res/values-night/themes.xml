        <item name="colorPrimary">@color/for_dark</item>
        <item name="colorPrimaryVariant">@color/purple_button</item>
        <item name="colorOnPrimary">@color/purple_button</item>
        <item name="colorPrimaryContainer">@color/purple_button</item>
        <item name="colorOnPrimaryContainer">@color/red</item>
        <item name="colorPrimaryInverse">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/purple_button</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="android:textColor">@color/white</item>

        <!-- Customize your theme here. -->
